{
  "name": "express-book-store",
  "version": "0.0.1",
  "description": "Example book store api using Express + Typescript and Knex ORM + Objection.",
  "main": "app.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "cleanup": "rimraf dist",
    "build": "npm run cleanup && tsc --pretty",
    "start": "node dist/src/app.js",
    "dev": "tsnd --poll --respawn --watch src/**/*.{ts} src/app.ts",
    "kmig": "npx knex --knexfile knexfile.ts migrate:latest",
    "kroll": "npx knex --knexfile knexfile.ts migrate:rollback",
    "klist": "npx knex --knexfile knexfile.ts migrate:list"
  },
  "author": "restuwahyu13",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.27.1",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.0",
    "express-rate-limiter": "^1.3.1",
    "express-slow-down": "^1.4.0",
    "express-validator": "^6.13.0",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-transform": "0.0.2",
    "knex": "^0.95.12",
    "midtrans-node-client": "0.0.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "objection": "^2.2.17",
    "pg": "^8.7.1",
    "piscina": "^3.1.0",
    "throng": "^5.0.0",
    "ts-mailgun": "^0.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.22",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.0",
    "@types/eslint": "^7.28.2",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-slow-down": "^1.3.2",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "^4.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/ioredis": "^4.27.8",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.6",
    "@types/pg": "^8.6.1",
    "@types/rimraf": "^3.0.2",
    "@types/throng": "^5.0.3",
    "@typescript-eslint/parser": "^5.3.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^8.1.0",
    "eslint-config-standard": "^16.0.3",
